{
    "name": "angular",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host rktasks --proxy-config proxy.conf.json",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "build-prod": "npm run generate:schema && ng build --prod",
        "test": "ng test",
        "generate:schema": "openapi-generator-cli generate -i ../../../../api-definition/src/main/resources/static/V1/openapi.yaml -g typescript-angular -o src/app/api --additional-properties=prependFormOrBodyParameters=false"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^13.0.3",
        "@angular/cdk": "^13.1.1",
        "@angular/common": "~13.0.0",
        "@angular/compiler": "~13.0.0",
        "@angular/core": "~13.0.0",
        "@angular/forms": "~13.0.0",
        "@angular/platform-browser": "~13.0.0",
        "@angular/platform-browser-dynamic": "~13.0.0",
        "@angular/router": "~13.0.0",
        "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "bootstrap": "^5.1.3",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1",
        "primeicons": "^5.0.0",
        "primeng": "^13.0.1",
        "quill": "^1.3.7",
        "rxjs": "^7.4.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.0.4",
        "@angular/cli": "~13.0.4",
        "@angular/compiler-cli": "~13.0.0",
        "@openapitools/openapi-generator-cli": "^2.5.1",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "@types/quill": "^2.0.9",
        "jasmine-core": "~3.10.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "typescript": "~4.4.3"
    }
